{
  "configVersion": "v3",
  "app": {
    "appId": 1050695,
    "appName": "Quran",
    "appType": "app",
    "version": {
      "code": 1,
      "name": "1.0.0"
    },
    "icon": "icon.png",
    "vender": "mahmoudbahaa",
    "description": "Quran app for downloading and playing Quran Surahs"
  },
  "permissions": [
    "data:os.device.info",
    "device:os.ble",
    "device:os.local_storage"
  ],
  "runtime": {
    "apiVersion": {
      "compatible": "1.0.0",
      "target": "1.0.0",
      "minVersion": "1.0.0"
    }
  },
  "targets": {
    "gt": {
      "module": {
        "page": {
          "pages": [
            "page/start",
            "page/home",
            "page/about",
            "page/select",
            "page/player",
            "page/settings/home",
            "page/settings/fontSize",
            "page/settings/lang",
            "page/settings/chaptersLang",
            "page/file/home",
            "page/file/manager",
            "page/file/storageInfo",
            "page/file/edit",
            "page/file/view/text",
            "page/file/view/image",
            "page/file/view/hex"
          ]
        },
        "app-side": {
          "path": "app-side/index"
        },
        "setting": {
          "path": "setting/index"
        }
      },
      "platforms": [
        {
          "st": "r",
          "dw": 480
        },
        {
          "st": "s",
          "dw": 390
        },
        {
          "st": "b",
          "dw": 194
        }
      ]
    }
  },
  "i18n": {
    "en-US": {
      "appName": "Quran"
    },
    "ar-EG": {
      "appName": "القرآن"
    }
  },
  "defaultLanguage": "en-US",
  "debug": true
}
