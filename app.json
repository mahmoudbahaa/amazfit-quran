{
  "configVersion": "v3",
  "app": {
    "appId": 26640,
    "appName": "Quran",
    "appType": "app",
    "version": {
      "code": 1,
      "name": "1.0.0"
    },
    "icon": "icon.png",
    "vender": "zepp",
    "description": "Quran app for downloading and playing quran Surahs"
  },
  "permissions": [
    "data:os.device.info",
    "device:os.bg_service",
    "device:os.ble",
    "device:os.local_storage",

    "device:os.alarm",
    "device:os.notification"
  ],
  "runtime": {
    "apiVersion": {
      "compatible": "3.0.0",
      "target": "3.0.0",
      "minVersion": "3.0"
    }
  },
  "targets": {
    "gt": {
      "module": {
        "page": {
          "pages": [
            "page/index",
            "page/select",
            "page/confirm",
            "page/download",
            "page/player"
          ]
        },
        "app-side": {
          "path": "app-side/index"
        },
        "setting": {
          "path": "setting/index"
        },
        "app-service": {
          "services": ["app-service/player_service"]
        }
      },
      "platforms": [
        {
          "st": "r",
          "dw": 480
        }
      ],
      "designWidth": 480
    }
  },
  "i18n": {
    "en-US": {
      "appName": "Quran"
    },
    "ar-EG": {
      "appName": "القرآن"
    }
  },
  "defaultLanguage": "en-US"
}
